{
  "mcpServers": {
    "qudag-mcp": {
      "name": "QuDAG MCP Server",
      "description": "Model Context Protocol server for QuDAG - quantum-resistant DAG consensus, vault management, and P2P networking",
      "transport": "stdio",
      "command": "cargo",
      "args": ["run", "--manifest-path", "/workspaces/daa/qudag/qudag-mcp/Cargo.toml", "--example", "basic_server", "--", "--no-interactive"],
      "capabilities": {
        "tools": true,
        "resources": true,
        "prompts": false
      },
      "features": [
        "quantum-crypto",
        "dag-consensus",
        "vault-management",
        "p2p-networking",
        "dark-addressing",
        "exchange-operations",
        "business-plan-management"
      ],
      "metadata": {
        "version": "1.0.0",
        "protocolVersion": "0.1.0",
        "supportedTransports": ["stdio"],
        "dagTools": 12,
        "cryptoTools": 8,
        "vaultTools": 10,
        "networkTools": 9,
        "exchangeTools": 15,
        "systemResources": 25
      }
    }
  }
}